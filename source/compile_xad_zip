gcc  -Wall -O3  -mcrt=newlib -D__USE_INLINE__ -DNDEBUG   -c -o extheader.o extheader_os4.c
gcc  -Wall -O3  -mcrt=newlib -D__USE_INLINE__ -DNDEBUG   -c -o xad_zip.o xad_zip.c
gcc  -Wall -O3  -mcrt=newlib -D__USE_INLINE__ -DNDEBUG   -c -o common.o common.c
gcc  -Tdata=0 -O3 -mcrt=newlib -nostartfiles -o Zip extheader.o common.o xad_zip.o -larchive
;copy Zip libs:xad
expunge all

