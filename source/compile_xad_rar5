gcc  -Wall -O3  -mcrt=newlib -D__USE_INLINE__ -DNDEBUG   -c -o extheader.o extheader_os4.c
gcc  -Wall -O3  -mcrt=newlib -D__USE_INLINE__ -DNDEBUG   -c -o xad_rar5.o xad_rar5.c
gcc  -Wall -O3  -mcrt=newlib -D__USE_INLINE__ -DNDEBUG   -c -o common.o common.c
gcc  -Tdata=0 -O3 -mcrt=newlib -nostartfiles -o RAR5 extheader.o common.o xad_rar5.o -larchive -lz -llzma -lbz2
;strip RAR5
;copy RAR5 libs:xad
expunge all

